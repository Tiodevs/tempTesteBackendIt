generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Blogs {
  id           String   @id @default(uuid()) @unique// ID único com UUID
  titulo       String
  texto        String   
  Banner       String  
  createdAt    DateTime @default(now())
  active       Boolean  @default(true) // Se está ativo na empresa
}

model Leads {
  id           String   @id @default(uuid()) @unique// ID único com UUID
  nome       String
  email        String   
  numero       String  
  createdAt    DateTime @default(now())
}


